{
    "manifest_version": 3,
    "name": "Minimal Chrome Extension",
    "version": "1.0",
    "description": "A minimal Chrome extension with browsing history permission and a popup UI.",
    "permissions": [
      "history"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }